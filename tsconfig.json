{
  "compilerOptions": {
      "outDir": "./dist",
      "target": "es6",
      "moduleResolution": "Node",
      "module": "commonjs",
      "sourceMap": true,
      "noImplicitAny": true,
      "typeRoots": ["node_modules/@0xproject/typescript-typings/types", "node_modules/@types"],
      "strictNullChecks": true,
      "lib": ["es2017"],
      "skipLibCheck": true,
      "allowJs": true
  },
  "include": [
      "src/**/*"
  ]
}